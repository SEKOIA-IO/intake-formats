[
  {
    "value": "Access status {azure.entraid.properties.conditionalAccessStatus} for user {user.name} from {source.ip} and risk state {azure.entraid.properties.riskState}",
    "conditions": [
      { "field": "azure.entraid.properties.conditionalAccessStatus" },
      { "field": "azure.entraid.properties.riskState" },
      { "field": "user.name" },
      { "field": "source.ip" }
    ]
  },
  {
    "value": "Event from {event.dataset} for user {user.name} from {source.ip}",
    "conditions": [
      { "field": "event.dataset" },
      { "field": "user.name" },
      { "field": "source.ip" }
    ]
  },
  {
    "value": "User {user.email} performed activity {service.name} from {source.ip}",
    "conditions": [
      { "field": "service.name" },
      { "field": "user.email" },
      { "field": "source.ip" }
    ]
  },
  {
    "value": "User with id {user.id} performed activity {service.name} from {source.ip}",
    "conditions": [
      { "field": "service.name" },
      { "field": "user.id" },
      { "field": "source.ip" }
    ]
  },
  {
    "value": "{event.outcome} event in {service.name} initiated by {azure.entraid.properties.app.displayName}",
    "conditions": [
      { "field": "service.name" },
      { "field": "event.outcome" },
      { "field": "azure.entraid.properties.app.displayName" }
    ]
  },
  {
    "value": "New event in {service.name}",
    "conditions": [{ "field": "service.name" }]
  }
]
