[
  {
    "value": "Access status {azure.entraid.properties.conditionalAccessStatus} for user {user.name} from {source.ip} and risk state {azure.entraid.properties.riskState}",
    "conditions": [
      { "field": "azure.entraid.properties.conditionalAccessStatus" },
      { "field": "azure.entraid.properties.riskState" },
      { "field": "user.name" },
      { "field": "source.ip" }
    ]
  },
  {
    "value": "Event from {event.dataset} for user {user.name} from {source.ip}",
    "conditions": [
      { "field": "event.dataset" },
      { "field": "user.name" },
      { "field": "source.ip" }
    ]
  },
  {
    "value": "Operation {event.action} with status {event.outcome} on {event.dataset} by user {user.name}",
    "conditions": [
      { "field": "event.action" },
      { "field": "event.outcome" },
      { "field": "event.dataset" },
      { "field": "user.name" }
    ]
  },
  {
    "value": "Operation {event.action} with status {event.outcome} on {event.dataset} by user {user.email}",
    "conditions": [
      { "field": "event.action" },
      { "field": "event.outcome" },
      { "field": "event.dataset" },
      { "field": "user.email" }
    ]
  },
  {
    "value": "Operation {event.action} with status {event.outcome} on {event.dataset}",
    "conditions": [
      { "field": "event.action" },
      { "field": "event.outcome" },
      { "field": "event.dataset" }
    ]
  }
]
