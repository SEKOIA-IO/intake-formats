[
  {
    "value": "{event.code} from {source.ip} to {destination.ip}, virtual server {citrix.adc.virtual_server.ip}, NAT {source.nat.ip}",
    "conditions": [
      { "field": "source.ip" },
      { "field": "destination.ip" },
      { "field": "citrix.adc.virtual_server.ip" },
      { "field": "source.nat.ip" },
      { "field": "event.dataset", "value": "audit_connection" }
    ]
  },
  {
    "value": "{event.code} from {source.ip} to {destination.ip}",
    "conditions": [
      { "field": "source.ip" },
      { "field": "destination.ip" },
      { "field": "event.dataset", "value": "audit_connection" }
    ]
  },
  {
    "value": "{event.code} from user {user.name} and IP {client.ip}",
    "conditions": [
      { "field": "event.code" },
      { "field": "user.name" },
      { "field": "client.ip" },
      { "field": "event.dataset", "value": "audit_sslvpn" }
    ]
  },
  {
    "value": "{event.reason}",
    "conditions": [
      { "field": "event.reason" },
      { "field": "event.dataset", "value": "audit_sslvpn" }
    ]
  },
  {
    "value": "{event.code} from {source.ip} to {destination.ip}",
    "conditions": [
      { "field": "source.ip" },
      { "field": "destination.ip" },
      { "field": "event.dataset", "value": "audit_sslvpn" }
    ]
  },
  {
    "value": "{event.code} observed by {observer.name}",
    "conditions": [
      { "field": "event.dataset", "value": "audit_snmp" },
      { "field": "event.code" },
      { "field": "observer.name" }
    ]
  },
  {
    "value": "{event.code} from {source.ip} with {tls.cipher}",
    "conditions": [
      {
        "field": "event.dataset",
        "value": "audit_ssl"
      },
      { "field": "event.code" },
      { "field": "source.ip" },
      { "field": "tls.cipher" }
    ]
  },
  {
    "value": "Connection from {source.ip} to {url.original} not blocked: {event.reason}",
    "conditions": [
      {
        "field": "event.action",
        "value": "not blocked"
      },
      {
        "field": "source.ip"
      },
      {
        "field": "url.original"
      },
      {
        "field": "event.reason"
      }
    ],
    "relationships": [
      {
        "source": "source.ip",
        "target": "url.original",
        "type": "connection not blocked"
      }
    ]
  },
  {
    "value": "Connection from {source.ip} to {url.original} blocked: {event.reason}",
    "conditions": [
      {
        "field": "event.action",
        "value": "blocked"
      },
      {
        "field": "source.ip"
      },
      {
        "field": "url.original"
      },
      {
        "field": "event.reason"
      }
    ],
    "relationships": [
      {
        "source": "source.ip",
        "target": "url.original",
        "type": "connection blocked"
      }
    ]
  },
  {
    "value": "{user.name} executed a shell command on {citrix.adc.pseudo_tty}: {process.command_line}",
    "conditions": [
      { "field": "user.name" },
      { "field": "citrix.adc.pseudo_tty" },
      { "field": "process.command_line" },
      { "field": "event.action", "value": "execute_shell_command" }
    ]
  },
  {
    "value": "{user.name} executed a shell command: {process.command_line}",
    "conditions": [
      { "field": "user.name" },
      { "field": "process.command_line" },
      { "field": "event.action", "value": "execute_shell_command" }
    ]
  },
  {
    "value": "{source.ip} launched a {http.request.method} request to {url.original}",
    "conditions": [
      { "field": "source.ip" },
      { "field": "http.request.method" },
      { "field": "url.original" },
      { "field": "event.dataset", "value": "audit_http_access" }
    ]
  },
  {
    "value": "Authentication rejected for {user.name} from {source.ip} to {destination.ip}",
    "conditions": [
      { "field": "event.category", "value": "authentication" },
      { "field": "event.type", "value": "start" },
      { "field": "event.outcome", "value": "failure" },
      { "field": "user.name" },
      { "field": "source.ip" },
      { "field": "destination.ip" }
    ]
  },
  {
    "value": "{event.code} event from {source.ip} ended with {event.outcome}",
    "conditions": [
      { "field": "event.code" },
      { "field": "source.ip" },
      { "field": "event.outcome" }
    ]
  },
  {
    "value": "{event.code} for {client.ip}: {event.reason}",
    "conditions": [
      { "field": "event.code" },
      { "field": "client.ip" },
      { "field": "event.reason" }
    ]
  },
  {
    "value": "AAATM event: {event.reason}",
    "conditions": [
      { "field": "event.reason" },
      { "field": "event.dataset", "value": "audit_aaatm" }
    ]
  },
  {
    "value": "{event.code}: {event.reason}",
    "conditions": [{ "field": "event.code" }, { "field": "event.reason" }]
  }
]
