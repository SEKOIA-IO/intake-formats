[
  {
    "value": "{event.reason} from {source.ip}",
    "conditions": [
      { "field": "event.action", "value": "incident_created" },
      { "field": "event.reason" },
      { "field": "source.ip" }
    ]
  },
  {
    "value": "Breadcrumb activity from {source.ip} on {Trapster.channel}",
    "conditions": [
      { "field": "event.action", "value": "incident_updated" },
      { "field": "Trapster.channel", "value": "breadcrumb" },
      { "field": "source.ip" },
      { "field": "Trapster.channel" }
    ]
  },
  {
    "value": "Honeytoken accessed by {source.ip}",
    "conditions": [
      { "field": "event.action", "value": "incident_updated" },
      { "field": "Trapster.channel", "value": "honeytoken" },
      { "field": "source.ip" },
      { "field": "Trapster.channel" }
    ]
  },
  {
    "value": "Incident update: {Trapster.category} on {Trapster.channel} from {source.ip}",
    "conditions": [
      { "field": "event.action", "value": "incident_updated" },
      { "field": "Trapster.category" },
      { "field": "Trapster.channel" },
      { "field": "source.ip" }
    ]
  },
  {
    "value": "Trapster device {Trapster.device.name} disconnected",
    "conditions": [
      { "field": "Trapster.category", "value": "disconnected" },
      { "field": "Trapster.device.name" }
    ]
  },
  {
    "value": "Test incident",
    "conditions": [{ "field": "Trapster.channel", "value": "test" }]
  },
  {
    "value": "Activity detected on {Trapster.channel} from {source.ip}:{source.port}",
    "conditions": [
      { "field": "Trapster.channel" },
      { "field": "source.ip" },
      { "field": "source.port" }
    ]
  }
]
